{
  "manifest_version": 2,

  "name": "Chrome password Plugin",
  "description": "This extension will protect you google chrome browser with password",
  "version": "1.0",

  "background": {
    "scripts": ["background.js"]
  },

  "content_scripts": [
    {
      "matches": ["*://*/*" ],
      "js": ["background.js"]
    }
  ],

  "browser_action": {
    "default_title": "Поставить пароль для chrome",
    "default_icon": "icon.ico",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab"
  ]
}